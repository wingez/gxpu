def main():

  val max_number = 30
  val number = 2
  while number < max_number:
    val factor = 2
    val is_prime = 1
    while factor < number:
      if mod(number, factor) == 0:
        is_prime = 0
        break
      factor = factor+1

    if is_prime>0:
      print(number)

    number = number+1

